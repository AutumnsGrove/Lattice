{
	"name": "grove-amber",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:ui": "vitest --ui",
		"lint": "eslint .",
		"format": "prettier --write .",
		"worker:dev": "wrangler dev --config worker/wrangler.toml",
		"worker:deploy": "wrangler deploy --config worker/wrangler.toml",
		"db:migrate": "wrangler d1 execute amber --file=./worker/migrations/schema.sql --config worker/wrangler.toml"
	},
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "^7.2.6",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@testing-library/jest-dom": "^6.4.0",
		"@testing-library/svelte": "^5.0.0",
		"@types/node": "^20.0.0",
		"@vitest/coverage-v8": "^2.0.0",
		"@vitest/ui": "^2.0.0",
		"eslint": "^9.0.0",
		"jsdom": "^24.0.0",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tslib": "^2.6.0",
		"typescript": "^5.0.0",
		"vite": "^5.0.0",
		"vitest": "^2.0.0",
		"tailwindcss": "^3.4.19",
		"wrangler": "^3.0.0"
	},
	"dependencies": {
		"@autumnsgrove/lattice": "workspace:*",
		"@cloudflare/workers-types": "^4.0.0",
		"@lucide/svelte": "^0.562.0",
		"bits-ui": "^2.14.4"
	}
}
