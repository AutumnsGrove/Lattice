{
  "name": "@autumnsgrove/vineyard",
  "version": "0.0.1",
  "description": "Svelte 5 component library for Grove ecosystem showcase/documentation pages",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./vineyard": {
      "types": "./dist/components/vineyard/index.d.ts",
      "svelte": "./dist/components/vineyard/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.50.2",
    "@sveltejs/package": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^4.0.4",
    "@testing-library/svelte": "^5.3.1",
    "@vitest/ui": "^2.1.9",
    "eslint": "^9.39.2",
    "jsdom": "^25.0.1",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "publint": "^0.2.12",
    "svelte": "^5.50.1",
    "svelte-check": "^4.3.6",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vitest": "^2.1.9"
  },
  "dependencies": {
    "lucide-svelte": "^0.460.1"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "svelte",
    "svelte5",
    "components",
    "grove",
    "documentation",
    "showcase"
  ],
  "author": "AutumnsGrove",
  "license": "MIT"
}
