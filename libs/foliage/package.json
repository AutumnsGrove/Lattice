{
	"name": "@autumnsgrove/foliage",
	"version": "0.1.0",
	"description": "Theme system for Grove â€” personal expression with modern guardrails",
	"author": "AutumnsGrove",
	"homepage": "https://github.com/AutumnsGrove/Foliage#readme",
	"bugs": {
		"url": "https://github.com/AutumnsGrove/Foliage/issues"
	},
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./themes": {
			"types": "./dist/themes/index.d.ts",
			"default": "./dist/themes/index.js"
		},
		"./components": {
			"types": "./dist/components/index.d.ts",
			"svelte": "./dist/components/index.js",
			"default": "./dist/components/index.js"
		},
		"./server": {
			"types": "./dist/server/index.d.ts",
			"default": "./dist/server/index.js"
		}
	},
	"svelte": "./dist/index.js",
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"test": "vitest",
		"test:run": "vitest run",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"lint": "eslint .",
		"format": "prettier --write ."
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
