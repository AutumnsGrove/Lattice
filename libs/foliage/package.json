		"@typescript-eslint/parser": "^8.0.0",
		"eslint": "^9.0.0",
		"eslint-plugin-svelte": "^2.0.0",
		"globals": "^16.5.0",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-eslint-parser": "^1.4.1",
		"typescript": "^5.0.0",
		"vite": "^6.0.0",
		"vitest": "^2.0.0"
	},
	"keywords": [
		"grove",
		"themes",
		"svelte",
		"css",
		"customization"
