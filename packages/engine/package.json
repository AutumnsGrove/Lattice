  "scripts": {
    "dev": "vite dev",
    "dev:wrangler": "wrangler pages dev -- vite dev",
    "build": "vite build",
    "build:sveltekit": "vite build",
    "build:package": "svelte-kit sync && svelte-package -o dist",
    "prepackage": "npx tsx ../../scripts/generate/grove-term-manifest.ts",
    "package": "svelte-kit sync && svelte-package -o dist",
    "prepublishOnly": "pnpm run package",
    "preview": "vite preview",
    "deploy": "wrangler pages deploy .svelte-kit/cloudflare --project-name grove-lattice",
    "audit": "pnpm audit --audit-level=moderate",
    "audit:fix": "pnpm audit --fix",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:security": "vitest run tests/security",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "email:dev": "email dev -p 3001 --dir src/lib/email",
    "email:export": "email export --dir src/lib/email"
  },
